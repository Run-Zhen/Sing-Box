  if (['Proxy'].includes(i.tag)) {
    i.outbounds.push(...getTags(proxies))
  }
  if (['ğŸ‡­ğŸ‡° Hong Kong'].includes(i.tag)) {
    i.outbounds.push(...getTags(proxies, /æ¸¯|hk|hong kong|Hong Kong|ğŸ‡­ğŸ‡°/i))
  }
  if (['ğŸ‡¹ğŸ‡¼ Taiwan'].includes(i.tag)) {
